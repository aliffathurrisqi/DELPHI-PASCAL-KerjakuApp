program Kerjaku;

uses
  Vcl.Forms,
  UnitLogin in 'UnitLogin.pas' {fLogin},
  UnitMenu in 'UnitMenu.pas' {Form1};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TfLogin, fLogin);
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
